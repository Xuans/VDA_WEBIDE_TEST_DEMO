   </el-form-item>\r\n        <el-form-item label=\"传参\">\r\n          <el-input placeholder=\"请输入JSON字符串\" v-model=\"form.params\" clearable></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"类型\">\r\n          <el-radio-group v-model=\"form.type\">\r\n            <el-radio label=\"BLANK\">新页面</el-radio>\r\n            <el-radio label=\"SUB\">子页面</el-radio>\r\n            <el-radio label=\"SELF\">自身页面</el-radio>\r\n            <el-radio label=\"WINDOW\">新窗口页面</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"openPage\">打开</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nimport  {mixins} from '@v2-lib/vue.spa.plugin';\r\nexport default {\r\n  mixins:[mixins],\r\n    data(){\r\n        return {\r\n            form:{\r\n                page:'',\r\n                type:'BLANK',\r\n                path:'',\r\n                params:'',\r\n                title:'',\r\n                other:false\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        beforeClose(){\r\n          console.log(\"open\")\r\n         return  true;\r\n           \r\n        },\r\n        openPage(){\r\n            let config = this.form;\r\n             \r\n            this.open({\r\n                status:true,\r\n                title:config.title,\r\n                page:config.page,\r\n                path:config.path,\r\n                type:config.type,\r\n                other:config.other,\r\n                params:config.params ? JSON.parse(config.params):'',\r\n                confirmCallback:() => {\r\n                  console.log('点击确定')\r\n                },\r\n                cancelCallback:(e) => {\r\n                  console.log('点击取消')\r\n                }\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.aweb-open-page {\r\n  padding: 12px;\r\n}\r\n\r\n.aweb-open-page-form {\r\n  padding: 20px;\r\n  margin: 0 20px;\r\n  // padding: 12px;\r\n // border: 1px solid #ebebeb;\r\n  border-radius: 3px;\r\n  transition: 0.2s;\r\n  form {\r\n    width: 600px;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/spa"}]}